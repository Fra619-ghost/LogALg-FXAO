Proceso ejercicio3
	Definir numero, sumaCuadrados como Entero;
	
    
    sumaCuadrados <- 0;
	
    
    Para numero <- 1 Hasta 100 Con Paso 1 Hacer
        sumaCuadrados <- sumaCuadrados + numero * numero;
    FinPara
	
    
    Escribir "La suma de los cuadrados de los números entre 1 y 100 es: ", sumaCuadrados;
	
	
FinProceso
